{
  "manifest_version": 2,
  "name":"To-Do List",
  "description": "To-Do List that help user to remaind their task",
  "version": "2.0.2",
  "icons": {"128": "icon_128.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts":["background.js"],
    "persistent": true
  },
  "permissions": ["activeTab", "storage", "notifications"]
}